<my-scene 
disable-scroll
    id="main-frame"
    width="{{width}}"
    height="{{height}}"
    style="width:100%;height:100%;display:block;" id="xr-scene" bind:ready="handleReady">
  <my-assets bind:progress="handleAssetsProgress" bind:loaded="handleAssetsLoaded">
    <my-asset-load type="gltf" asset-id="gltf-just_a_girl" src="https://mmbizwxaminiprogram-1258344707.cos.ap-guangzhou.myqcloud.com/xr-frame/demo/just_a_girl/index.glb" />
  </my-assets>
  <my-node>
    <my-node node-id="camera-target" position="0 0 0"></my-node>
    <my-gltf node-id="gltf-just_a_girl" position="0 -0.5 0" rotation="0 0 0" scale="0.01 0.01 0.01" model="gltf-just_a_girl"></my-gltf>
    <my-camera
      id="camera" node-id="camera" position="0 0.4 1.6" clear-color="0.9 0.9 0.9 1"
      target="camera-target"
      camera-orbit-control=""
    ></my-camera>
  </my-node>
  <my-node node-id="lights">
    <my-light type="ambient" color="1 1 1" intensity="0.3" />
    <my-light type="directional" rotation="180 0 0" color="1 1 1" intensity="2" />
  </my-node>
</my-scene>
